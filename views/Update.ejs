<div hx-get="/update" hx-trigger="every 55s" hx-swap="outerHTML" class="fiat-crypto-table table-responsive" style="margin: 0px auto;">

    <table class="table table-borderless text-center">
        <thead>
            <tr>
                <th>
                    <h4><span class="pointer">#</span></h4>
                </th>
                <th>
                    <h4><span class="pointer">Name</span></h4>
                </th>
                <th>
                    <h4><span class="pointer">Last</span></h4>
                </th>
                <th>
                    <h4><span class="pointer">Buy / Sell Price</span></h4>
                </th>
                <th>
                    <h4><span class="pointer">Volume</span></h4>
                </th>
                <th>
                    <h4><span class="pointer">base_unit</span></h4>
                </th>
            </tr>
        </thead>

        <tbody>


            <%for(let i = 0  ; i < data.length ; i++){%>
            <tr>
                <td class="align-middle">
                    <h4 class="table-text"><%= i+1 %></h4>
                </td>

                <td class="align-middle">
                    <a target="_blank"
                        href="https://wazirx.com/invite/sp7pvbt6?utm_source=finstreet&amp;utm_medium=affiliate&amp;utm_campaign=regnow-btn">
                        <h4 class="table-text">
                            <span class="exchange-name "><%=data[i].name%></span>
                        </h4>
                    </a>
                </td>

                <td class="align-middle">
                    <h4 class="table-text">₹ <%=data[i].last%></h4>
                </td>

                <td class="align-middle">
                    <h4 class="table-text"><span>₹ <%=data[i].buy%> / ₹ <%=data[i].sell%></span></h4>
                </td>

                <td class="align-middle">
                    <h4 class="table-text"> <%=data[i].volume%></h4>
                </td>

                <td class="align-middle">
                    <h4 class="table-text"> <%=data[i].base_unit%></h4>
                </td>

            </tr>
            <%}%>
             
        </tbody>
    </table>

</div>